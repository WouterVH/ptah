<h5>
  <a href="../source.html?pkg=${factory.__module__}#sl-${lineno(factory)}">
    ${factoryInfo}
  </a>
</h5>
<p>
  listen to:
  <div class="span10">
    <tal:block condition="obj">
      <tal:block tal:content="obj" /><br />
    </tal:block>

    <a tal:condition="event" href="../events.html?ev=${event.name}">
      ${event.title}</a><br/>
    <tal:block tal:condition="not event" tal:content="klass" />
  </div>

  <tal:block condition="factory.__doc__">
    <br />
    <em tal:content="factory.__doc__" />
  </tal:block>
</p>
