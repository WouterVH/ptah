<div class="page-header">
  <h2>SQL Tables</h2>
</div>

<div class="row">
  <div class="span16 column">
    <table class="zebra-striped">
      <tr tal:repeat="tb view.tables">
	<td>
	  <h4>
	    <a href="${tb[0]}/">${tb[0]}</a>
	  </h4>
	  <p>
	    <tal:block tal:repeat="rec tb[1][1]">
	      ${rec}<br />
	    </tal:block>
	  </p>
	</td>
      </tr>
    </table>
  </div>
</div>
